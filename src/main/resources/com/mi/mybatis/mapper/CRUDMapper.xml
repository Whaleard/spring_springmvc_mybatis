<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mi.mybatis.mapper.CRUDMapper">

    <insert id="insertEmployee">
        insert into t_employee values (null, 'admin', '123456', 23, '男', '12345@qq.com')
    </insert>

    <update id="updateEmployee">
        update t_employee set username = '张三' where id = 11
    </update>

    <delete id="deleteEmployee">
        delete from t_employee where id = 7
    </delete>

    <!--
        查询功能的标签必须设置resultType或resultMap
            resultType：设置默认的映射关系
            resultMap：设置自定义的映射关系
     -->
    <select id="getEmployeeById" resultType="com.mi.mybatis.entity.Employee">
        select * from t_employee where id = 9
    </select>

    <select id="getAllUser" resultType="Employee">
        select * from t_employee
    </select>
</mapper>